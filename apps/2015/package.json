{
	"name": "@simonporter007/advent-of-code-2015",
	"scripts": {
		"start": "aocrunner day",
		"build": "aocrunner build",
		"update:readme": "aocrunner update:readme",
		"prettier": "prettier --check './**/*.{js,jsx,ts,tsx}' --cache",
		"prettier:fix": "npm run prettier -- --write",
		"typecheck": "tsc --noEmit",
		"lint": "eslint --quiet --ext .js,.jsx,.ts,.tsx . && npm run prettier",
		"lint:fix": "TIMING=1 eslint --quiet --fix \"src/**/*.ts*\" && npm run prettier:fix",
		"clean": "rm -rf .turbo && rm -rf dist && rm -rf .next && rm -rf .swc",
		"test": "jest --passWithNoTests --silent"
	},
	"dependencies": {
		"@simonporter007/eslint-config": "workspace:^",
		"@simonporter007/tsconfig-config": "workspace:^",
		"@simonporter007/utils": "workspace:^",
		"aocrunner": "^1.9.6",
		"esbuild": "^0.19.8",
		"jest": "^29.7.0",
		"n-readlines": "^1.0.1",
		"prettier": "^3.1.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"ts-md5": "^1.3.1",
		"typescript": "^5.3.2"
	},
	"devDependencies": {
		"@types/n-readlines": "^1.0.6",
		"@types/node": "^20.10.1",
		"@types/react": "^18.2.39",
		"@types/react-dom": "^18.2.17"
	},
	"type": "module"
}